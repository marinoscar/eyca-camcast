@model eyca.web.Models.ImageInfo
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_AddItem.cshtml";
}

<div class="page-header">
    <h1>Enter the information for the Robot</h1>
    <h3>You are about to process image @Model</h3>
</div>
<form id="contact-form" class="form-horizontal" method="post" action="/Invoice/Process" role="form">
    @Html.Partial("_Contact")
    <input type="hidden" id="imageId" name="ImageId" value="@Model" />
    <div style="height:25px;"></div>
    <div class="form-group form-group-lg">
        <button type="submit" class="btn btn-lg btn-defaul btn-custom">Submit</button>
        <a href="/Invoice/Home" class="btn btn-default btn-lg">Return</a>
    </div>
</form>

